    "jiti": "^2.6.1",
    "mitata": "^1.0.34",
    "oxc-minify": "^0.64.0",
    "oxc-transform": "^0.65.0",
    "oxfmt": "^0.16.0",
    "stnl": "./lib",
    "typescript": "^5.9.3"
  }
}
